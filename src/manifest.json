{
    "manifest_version": 3,
    "name": "Chirag",
    "version": "1.0.0",
    "description": "Mock API responses by having your requests intercepted and responded with your chosen response.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "36": "icons/36.png",
        "48": "icons/48.png",
        "72": "icons/72.png",
        "96": "icons/96.png",
        "144": "icons/144.png",
        "192": "icons/192.png"
    },
    "action": {
        "default_icons": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "36": "icons/36.png",
            "48": "icons/48.png",
            "72": "icons/72.png",
            "96": "icons/96.png",
            "144": "icons/144.png",
            "192": "icons/192.png"
        },
        "default_popup": "index.html",
        "default_title": "Chirag - Mock API Responses in Browser"
    },
    "permissions": [
        "storage",
        "debugger",
        "activeTab"
    ],
    "background": {
        "service_worker": "service_worker.js"
    }
}